{
  "name": "gideon",
  "version": "0.1.0",
  "description": "feather like nodejs ts template with linting",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc --build tsconfig.prod.json",
    "start": "nodemon",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint --cache --fix  \"./src/**/*.+(js|ts|yml|yaml|json)\"",
    "format": "prettier --write \"./src/**/*.(js|ts|yml|yaml|json)\"",
    "prepare": "husky install"
  },
  "keywords": [
    "gideon",
    "featherjs",
    "like",
    "template",
    "for",
    "backend",
    "development"
  ],
  "author": "gideon ifeanyi mmeremnwanne",
  "license": "MIT",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/fs-extra": "^9.0.13",
    "@types/http-errors": "^1.8.1",
    "@types/jest": "^27.4.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.8",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "accesscontrol": "^2.2.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.3",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "helmet": "^5.0.1",
    "http-errors": "^2.0.0",
    "joi": "^17.5.0",
    "mongoose": "^6.1.7",
    "mongoose-errors": "^1.1.2",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "socket.io": "^4.4.1",
    "winston": "^3.4.0"
  },
  "lint-staged": {
    "*.{js,jsx,tsx,ts,scss,json,html}": [
      "npm run format",
      "npm run lint"
    ]
  }
}
